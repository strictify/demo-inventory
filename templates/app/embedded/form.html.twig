<div class="card">
    <div class="card-header hide-empty">{{ form_label(form) }}</div>
    <div class="card-body">
        {{ form_start(form, {attr: {'autocomplete': 'off'}}) }}
        {% block form %}
            {{ form_rest(form) }}
        {% endblock form %}

        <div class="card-footer">
            <button type="submit" class="btn btn-primary">Yo</button>
        </div>
        {{ form_end(form) }}
    </div>

</div>
